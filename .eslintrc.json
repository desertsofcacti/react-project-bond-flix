{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"prettier/prettier": "error"
	}
}
